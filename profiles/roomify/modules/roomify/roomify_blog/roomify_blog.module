<?php

/**
 * @file
 * Code for the Roomify Blog feature.
 */

include_once 'roomify_blog.features.inc';

/**
 * Implements hook_ctools_plugin_directory().
 */
function roomify_blog_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/content_types';
  }
}
